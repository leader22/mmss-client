declare type MusicJSON = Artist[];

declare type Artist = {
  name: string;
  albums: Album[];
};

declare type Album = {
  name: string;
  year: string;
  songs: Song[];
};

declare type Song = {
  name: string;
  artist: string;
  album: string;
  disc: string;
  track: string;
  duration: string;
  path: string;
};
